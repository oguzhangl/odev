var i,j,sayac,a;	
 for (i=1;i<=100;i++){
 sayac=0;	                                                            
 (i%2==0)																
 a="çift";															  
 else 																	
 a="tek"; 
 for (j=1;j<=i;j++)  {
         if (i%j==0)
             sayac+=1;
     }
    if (sayac==2)
         console.log(i, "asal", a);
    else
          console.log(i, "asal değil", a);
        
  }
